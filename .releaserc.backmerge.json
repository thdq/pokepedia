{
  "branches": ["master"],
  "plugins": [
    [
      "@kilianpaquier/semantic-release-backmerge",
      {
        "commit": "chore(release): merge branch ${from} into ${to} [skip ci]",
        "targets": [
          {
            "from": "master",
            "to": "development"
          }
        ]
      }
    ]
  ]
}

